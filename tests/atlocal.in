# top source directory
abs_top_srcdir=@abs_top_srcdir@
export abs_top_srcdir

# Set the path to a good sed.
if test -z "$SED"
then
    SED=sed
    export SED
fi

# Set the path to the uninstalled data files of the tool set
if test -z "$CREOL_LIBRARY_PATH"
then
    CREOL_LIBRARY_PATH=@abs_top_srcdir@/share
fi
export CREOL_LIBRARY_PATH

if test -z "$CREOLC"
then
  CREOLC=creolc
fi
export CREOLC
 

# Modify the path of maude to find our interpreter definitions
if test -z "$MAUDE_LIB"
then
    MAUDE_LIB="$CREOL_LIBRARY_PATH"
else
    MAUDE_LIB="$CREOL_LIBRARY_PATH":$MAUDE_LIB
fi
export MAUDE_LIB

if test -z "$MAUDE"
then
  MAUDE=@MAUDE@
  export MAUDE
fi

WITH_METATESTS=@WITH_METATESTS@

if test "x@BACKEND_XML@" != "x"
then
    WITH_XML=true
else
    WITH_XML=false
fi
export WITH_XML
