new interface Transaction
begin
with Any
op transaction(in s:Int, s:String)
end


update BankAccount implements Transaction 
 begin
  
   var logger : Log

   with Any

   op transaction(in sum: Int, s:String) ==  bal:= (bal + sum); logger := new Log(); logger.log(s;)

 end


